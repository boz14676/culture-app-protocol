{
    "meta":
    {
        "title"     : "Culture API",
        "output"    : "music-api",
        "author"    : "Taylor",
        "source"    : "none",
        "prefix"    : ""
    },

  	"enum":
  	{
        // 主题类型
        "MUSICABLE_TYPE": {
            "ARTICLE_CATEGORY": "article_category",     // 文章列表主题
            "ARTICLE": "article"                        // 文章主题
        }
  	},

    "model":
    {
        // 视频对象
        "MUSIC": {
            "id": "",
            "name": "",                             // 名称
            "thumbnail": "",                        // 缩略图
            "url": "",                              // 地址
            "lenstening_numbers": "",               // 观看次数
            "singer_name": "",                      // 歌手名称

            "videoable_type": "<VIDEOABLE_TYPE>",   // 主题类型 *仅用于搜索时使用
            "videoable_id": ""                      // 主题ID *仅用于搜索时使用
        }
    },

    "controller":
    {
        // 获取视频(s)
        "GET /v1/musics":
        {
            "request":
            {
                "page": "1",                             // *当前页数
                "per_page": "10",                        // *每页显示的数量
                "q[attribute_name]": "",                 // 搜索
                "s[attribute_name]": "<SORT_ORDER>"      // 排序
            },
            "response":
            {
                "data": ["{MUSIC}"]
            }
        }
    }
}
